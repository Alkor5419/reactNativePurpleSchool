{
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit",
        "source.organizeImports": "never"
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "eslint.codeActionsOnSave.mode": "all",
    "editor.formatOnSave": false, 
    "files.autoSave": "onFocusChange",
    
}
